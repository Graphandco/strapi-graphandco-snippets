{
   "kind": "collectionType",
   "collectionName": "categories",
   "info": {
      "singularName": "category",
      "pluralName": "categories",
      "displayName": "categories"
   },
   "options": {
      "draftAndPublish": true
   },
   "pluginOptions": {},
   "attributes": {
      "name": {
         "type": "string",
         "required": true,
         "unique": true
      },
      "slug": {
         "type": "uid",
         "targetField": "name"
      },
      "color": {
         "type": "string",
         "default": "#3b82f6"
      },
      "snippets": {
         "type": "relation",
         "relation": "oneToMany",
         "target": "api::snippet.snippet",
         "mappedBy": "category"
      }
   }
}
